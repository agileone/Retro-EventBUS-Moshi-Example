<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hammad13060.androidclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.GetAllUsersActivity" />
        <activity android:name=".activities.GetUserActivity" />
        <activity android:name=".activities.AddUserActivity" />
        <activity android:name=".activities.DeleteUserActivity" />
        <activity android:name=".activities.UpdateUserActivity"></activity>
    </application>

</manifest>
